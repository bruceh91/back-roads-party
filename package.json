{
  "name": "in-class-db-api",
  "version": "1.0.0",
  "description": "",
  "main": "server/lib/index.js",
  "scripts": {
    "build:client": "webpack",
    "build:server": "babel ./server/src -d ./server/lib",
    "dev": "source ENV_VAR && concurrently -k \"npm run watch:client\" \"npm run watch:server\" \"nodemon --watch server/lib --delay 1\"",
    "prepare": "npm run build:server",
    "start": "npm run build:client && npm run build:server && npm run start:server && npm run dev",
    "start:server": "node server/lib/index.js",
    "watch:client": "webpack --watch",
    "watch:server": "babel ./server/src -w -d ./server/lib"
  },
  "author": "Bruce Harris",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.251.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "^1.0.3",
    "express": "^4.16.3",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "npm": "^6.1.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "react": "^16.3.2",
    "react-day-picker": "^7.1.6",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "uuid": "^3.2.1",
    "webpack": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.3"
  }
}
